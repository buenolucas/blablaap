{
  "name": "blablaap",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "labs/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@blablaap/config": "workspace:*",
    "@blablaap/eslint-config": "workspace:*",
    "@changesets/changelog-github": "^0.4.4",
    "@changesets/cli": "^2.22.0",
    "@commitlint/cli": "^16.2.4",
    "@jest/types": "^27.4.2",
    "@rushstack/eslint-patch": "^1.1.3",
    "eslint": "7.32.0",
    "husky": "^7.0.4",
    "jest": "^28.0.3",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "tsup": "^5.11.11",
    "turbo": "latest",
    "typescript": "^4.5.3"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.0.1"
}